				"DEVC",
				"INTF"
			],
			"exclude": [],
			"severity": "Error"
		},
		"ambiguous_statement": {
			"exclude": [],
			"severity": "Error"
		},
		"avoid_use": {
			"break": true,
			"defaultKey": false,
			"define": true,
			"describeLines": true,
			"exclude": [],
